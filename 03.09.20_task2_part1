#include <iostream>

using namespace std;

int main()
{
    int N,M;
    cout<<"Enter length and weidth of rectangle"<<endl;
    cin>>N;
    cin>>M;
    for (int i=0; i<N; i++) {
        cout<<"*";
        for (int j=1; j<M; j++) {
            cout<<"*";
        }
        cout<<endl;
    }

    return 0;
}
